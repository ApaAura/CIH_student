<Window x:Class="Problema_5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Problema_5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <TextBlock DockPanel.Dock="Top" Background="LightBlue" Padding="10"> DockPanel.Dock="Top"</TextBlock>
        <TextBlock DockPanel.Dock="Top" Background="#FF579EB5" Padding="10"> Toolbar</TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Background="#FF3E5456" Padding="10"> DockPanel.Dock="Bottom"</TextBlock>
        <TextBlock DockPanel.Dock="Right" Background="#FF5CA4A4" Padding="10"> DockPanel.Dock="Right"</TextBlock>
        <TextBlock DockPanel.Dock="Left" Background="#FF05454A" Padding="10"> DockPanel.Dock="Left"</TextBlock>
    </DockPanel>
</Window>
